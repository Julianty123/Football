      <CheckBox fx:id="checkGuideTile" layoutX="363.0" layoutY="85.0" mnemonicParsing="false" onAction="#handleGuideTile" text="Guide Tile" />
      <RadioButton fx:id="radioButtonWalk" layoutX="371.0" layoutY="166.0" mnemonicParsing="false" text="&quot;Walk&quot;">
         <toggleGroup>
            <ToggleGroup fx:id="b" />
