                  <RadioButton fx:id="radioButtonTrap" layoutX="163.0" layoutY="114.0" mnemonicParsing="false" text="Trap [Key F2]" toggleGroup="$a" />
                  <RadioButton fx:id="radioButtonDribble" layoutX="29.0" layoutY="145.0" mnemonicParsing="false" text="Dribble [Key F3]" toggleGroup="$a" />
