      <Text fx:id="textBallCoords" layoutX="23.0" layoutY="67.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ball Coords: (0, 0)" textAlignment="CENTER" />
      <TextField fx:id="textBallID" editable="false" layoutX="25.0" layoutY="86.0" prefHeight="0.0" prefWidth="122.0" />
      <CheckBox fx:id="checkBall" layoutX="162.0" layoutY="91.0" mnemonicParsing="false" />
      <CheckBox fx:id="checkDisableDouble" layoutX="327.0" layoutY="20.0" mnemonicParsing="false" text="Disable double click" />
      <CheckBox fx:id="checkClickThrough" layoutX="352.0" layoutY="51.0" mnemonicParsing="false" onAction="#handleClickThrough" text="Click Through" />
